{
  "name": "esbuild-plugin-svgr",
  "version": "1.0.0",
  "description": "typescript starter",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.build.json --declaration",
    "prepublish": "npm run build",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint ./src --ext .ts"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-react-typescript": "^1.0.10",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-alias": "^1.2.0",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "@svgr/core": "^6.3.1",
    "esbuild": "^0.15.6"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "alias"
    }
  },
  "author": "Yulong Ruan <ruanyu1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ruanyl/esbuild-plugin-svgr.git"
  },
  "homepage": "https://github.com/ruanyl/esbuild-plugin-svgr",
  "bugs": "https://github.com/ruanyl/esbuild-plugin-svgr/issues"
}
